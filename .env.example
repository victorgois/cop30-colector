# Configurações do Banco de Dados PostgreSQL
# Para Supabase, use: postgresql://postgres:[SENHA]@db.[PROJETO].supabase.co:5432/postgres
# Para Render, use: postgresql://usuario:senha@dpg-xxxxx.render.com/nome_db
# Para desenvolvimento local: postgresql://usuario:senha@localhost:5432/cop30_db
DATABASE_URL=postgresql://usuario:senha@localhost:5432/cop30_db

# Configurações do Apify
APIFY_API_TOKEN=seu_token_apify_aqui

# Configurações do Servidor
PORT=3000
NODE_ENV=development

# Configurações de Coleta
# Coleta automática habilitada (true/false)
AUTO_COLLECT_ENABLED=true

# Configurações de Log
LOG_LEVEL=info
